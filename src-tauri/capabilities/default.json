{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    "dialog:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.roblox.com/"
        },
        {
          "url": "https://*.roproxy.com/"
        }
      ]
    },
    "store:allow-save",
    "store:allow-load",
    "store:allow-get-store",
    "store:allow-set",
    "store:default",
    "fs:default"
  ]
}